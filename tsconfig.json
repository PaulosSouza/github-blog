{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "plugins": [
      {
        "name": "@styled/typescript-styled-plugin"
      }
    ],
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "paths": {
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@pages/*": [
        "./src/pages/*"
      ],
      "@styles/*": [
        "./src/styles/*"
      ],
      "@layouts/*": [
        "./src/layouts/*"
      ],
      "@lib/*": [
        "./src/lib/*"
      ],
      "@contexts/*": [
        "./src/contexts/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ]
    }
  },
  "include": [
    ".eslintrc.cjs",
    "src"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}